<h1>Listing environments</h1>

<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
  </tr>

<% @graphs.each do |graph| %>
  <tr>
    <td><%= graph.name %></td>
    <td><%= link_to 'Edit', edit_project_graph_path(@project,graph) %></td>
    <td><%= link_to 'Destroy', project_graph_path(@project,graph), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Environment', new_project_graph_path(@project) %>
