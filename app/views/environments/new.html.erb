<%= render 'toolbar' %>
<div class="row">
  <%= render 'graph_draggables', resource_types: @resource_types %>
  <%= render 'graph_area' %>
  <%= render 'dialogs' , project: @project %>
  <%= render 'instance_templates', resource_types: @resource_types %>
</div>
<script type="text/javascript">
  prepareStage();
$(document).ready(function(){
  $('input#graph-name').val('<%= params[:environment]['name'] %>');
  $('input#graph-key-pair-name').val('<%= params[:environment]['key-pair-name'] %>');
  $('input#graph-security-group-name').val('<%= params[:environment]['group-name'] %>');
});
</script>
