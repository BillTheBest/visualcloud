<!-- This is where all the dialogs that popup on Environment drawing area reside -->

<!-- Save Environment -->
<%= render 'save_environment', key_pairs: @key_pairs, security_groups: @security_groups, project: project %>

<!-- Deployment Configuration dialog  -->
<%= render 'deployment_configuration', branch: @environment.branch ,db_migrate: @environment.db_migrate %>

<% %w(rails java php mysql nginx).each do |resource_type| %>
  <%= render 'ec2_type_resource_configuration', resource_type: resource_type %>
<% end %>

<!-- RDS resource  -->
<%= render 'rds_configuration' %>

<!-- ELB resource  -->
<%= render 'elb_configuration' %>

<!-- EC resource  -->
<%= render 'elasticache_configuration' %>

