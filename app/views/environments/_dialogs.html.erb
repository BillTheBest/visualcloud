<!-- This is where all the dialogs that popup on Environment drawing area reside -->

<!-- Save Environment -->
<%= render 'save_environment', key_pairs: @key_pairs, security_groups: @security_groups, project: project %>

<!-- Deployment Configuration dialog  -->
<%= render 'deployment_configuration', branch: @environment.branch %>

<!-- EC2 resource  -->
<%= render 'ec2_configuration' %>

<!-- Rails resource  -->
<%= render 'rails_configuration' %>

<!-- JAVA resource  -->
<%= render 'java_configuration' %>

<!-- PHP resource  -->
<%= render 'php_configuration' %>

<!-- Mysql resource  -->
<%= render 'mysql_configuration' %>

<!-- Nginx resource  -->
<%= render 'nginx_configuration' %>

<!-- RDS resource  -->
<%= render 'rds_configuration' %>

<!-- ELB resource  -->
<%= render 'elb_configuration' %>

<!-- EC resource  -->
<%= render 'elasticache_configuration' %>
