<% if flash.now[:error].nil? %>
  var newInstance = addInstanceCloneToGraph();
  newInstance.instance({xpos: <%= @instance.xpos %>, ypos:  <%= @instance.ypos %>, label: label, resourceType: 'EC2'});
  $('#ec2-configuration').modal('hide');
  addMessagesToDiv($('#global-messages'), '<%= escape_javascript flash_messages %>');
<% else %>
  addMessagesToDiv($('#ec2-config-error-messages'), '<%= escape_javascript flash_messages %>');
<% end %>
